package opgave2app;

import opgave2.Gift;
import opgave2.Person;

public class mainApp {
	public static void main(String[] args) {
		
	
		// TODO Auto-generated constructor stub
		Person person1 = new Person("Ole", 21);
		Person person2 = new Person("Niller", 18);
		Gift gift1 = new Gift("Transformer", 1000);
		Gift gift2 = new Gift("Nintendo Switch", 1000);

		person1.receives(gift1);
		person1.receives(gift2);

		System.out.println(person1.getTotalPriceOfGifts());
	}


}
