package opgave2;

public class Person {
	private String name;
	private int age;

	public Person(String name, int age) {
		// TODO Auto-generated constructor stub
		this.age = age;
		this.name = name;
	}
	
	public void setAge(int age) {
		this.age = age;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return super.toString();
	}
}
